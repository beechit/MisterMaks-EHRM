{
	"name": "beech/mister-maks-base-distribution",
	"description": "Mister Maks Base Distribution",
	"repositories": [
		{
			"type": "git",
			"url": "git://github.com/radmiraal/Radmiraal.CouchDB.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/TYPO3.CouchDB.git"
		},
		{
			"type": "composer",
			"url": "http://ci.beech.local:8080/job/composer-packages/ws/mistermaks/"
		},
		{
			"type": "composer",
			"url": "http://ci.typo3.robertlemke.net/job/composer-packages/ws/repository/"
		}
	],
	"config": {
		"vendor-dir":"Packages/Libraries",
		"bin-dir": "bin"
	},
	"require": {
		"typo3/flow": "dev-master",
		"typo3/eel": "dev-master",
		"typo3/form": "dev-master",
		"typo3/formbuilder": "dev-master",
		"typo3/tcpdf": "dev-master",
		"typo3/typoscript": "dev-master",
		"typo3/surf": "dev-master",
		"typo3/ldap": "dev-master",
		"typo3/twitter-bootstrap": "dev-master",
		"radmiraal/emberjs": "dev-master",
		"famelo/scaff": "dev-master",
		"typo3/expose": "dev-master",
		"beech/ehrm": "dev-development",
		"beech/party": "dev-development",
		"beech/task": "dev-development",
		"beech/calendar": "dev-development",
		"beech/cla": "dev-development",
		"beech/document": "dev-development",
		"beech/workflow": "dev-development",
		"beech/minutes": "dev-development",
