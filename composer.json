{
	"name": "beech/mister-maks-base-distribution",
	"description": "Mister Maks Base Distribution",

	"repositories": [
		{ "type": "git", "url": "git://github.com/beechit/Radmiraal.Emberjs.git" },
		{ "type": "git", "url": "git://github.com/beechit/TYPO3.Queue.git" },
