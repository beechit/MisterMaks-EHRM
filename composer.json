{
	"name": "beech/mister-maks-base-distribution",
	"description": "Mister Maks Base Distribution",
	"repositories": [
		{
			"type": "composer",
			"url": "http://ci.beech.local:8080/job/composer-packages/ws/mistermaks/"
		}
	],
	"config": {
		"vendor-dir":"Packages/Libraries",
		"bin-dir": "bin"
	},
	"extra": {
		"installer-paths": {
			"Packages/Beech/{$name}/": [
				"beech/dev-tools",
				"beech/style"
			]
		}
	},
	"require": {
		"php": ">=5.4.0",
		"typo3/flow": "dev-master",
		"typo3/eel": "*",
		"beech/essentials": "dev-master",
		"typo3/surf": "*",
		"typo3/ldap": "*",
		"radmiraal/emberjs": "dev-master",
		"radmiraal/couchdb": "dev-doctrine",
		"typo3/formbuilder": "*",
		"typo3/queue-redis": "*",
		"react/event-loop": "*",
		"typo3/usermanagement": "*"
	},
	"require-dev": {
		"typo3/buildessentials": "*",
		"typo3/kickstart": "*",
		"phpunit/phpunit": "3.7.*",
		"typo3-qa/typo3flow": "*",
		"mikey179/vfsstream": "1.1.*",
		"beech/style": "dev-development",
		"phpmd/phpmd": "1.4.*",
		"phpcd/phpcd": "dev-master",
		"pdepend/pdepend": "dev-master",
		"beech/dev-tools": "dev-development",
		"sandstorm/plumber": "dev-fixes-t3home13",
		"sandstorm/phpprofiler": "dev-fixes-t3home13 as 1.0.3-dev"
	},
	"minimum-stability": "dev",
	"scripts": {
		"post-update-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
		"post-install-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall"
	}
}