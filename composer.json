{
	"name": "beech/mister-maks-base-distribution",
	"description": "Mister Maks Base Distribution",
	"repositories": [
		{
			"type": "git",
			"url": "git://github.com/mneuhaus/Famelo.Gerrit.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/Radmiraal.CouchDB.git"
		},
		{
			"type": "composer",
			"url": "http://ci.beech.local:8080/job/composer-packages/ws/mistermaks/"
		},
		{
			"type": "composer",
			"url": "http://ci.typo3.robertlemke.net/job/composer-packages/ws/repository/"
		}
	],
	"config": {
		"vendor-dir":"Packages/Libraries",
		"bin-dir": "bin"
	},
	"require": {
		"typo3/flow": "*",
		"beech/ehrm": "1.0.x",
		"typo3/surf": "dev-master",
		"typo3/ldap": "dev-master",
		"radmiraal/emberjs": "dev-master",
		"radmiraal/couchdb": "dev-doctrine",
		"beech/essentials": "dev-master"
	},
	"require-dev": {
		"typo3/buildessentials": "dev-master",
		"famelo/gerrit": "dev-master",
		"typo3/kickstart": "dev-master",
		"mikey179/vfsstream": "1.1.*",
		"beech/style": "dev-development"
	},
	"minimum-stability": "dev",
	"scripts": {
		"post-update-cmd": "Beech\\Essentials\\Composer\\InstallerScripts::postUpdateAndInstall",
		"post-install-cmd": "Beech\\Essentials\\Composer\\InstallerScripts::postUpdateAndInstall"
	}
}
