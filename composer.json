{
	"name": "beech/mister-maks-base-distribution",
	"description": "Mister Maks Base Distribution",
	"repositories": [
		{
			"type": "git",
			"url": "git://github.com/radmiraal/phpcpd.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/TYPO3Flow.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/TYPO3SniffPool.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/Radmiraal.CouchDB.git"
		},
		{
			"type": "git",
			"url": "git://github.com/radmiraal/TYPO3.UserManagement.git"
		},
		{
			"type": "composer",
			"url": "http://ci.beech.local:8080/job/composer-packages/ws/mistermaks/"
		}
	],
	"config": {
		"vendor-dir":"Packages/Libraries",
		"bin-dir": "bin"
	},
	"require": {
		"php": ">=5.4.0",
		"typo3/flow": "dev-master",
		"beech/essentials": "dev-master",
		"typo3/surf": "dev-master",
		"typo3/ldap": "dev-master",
		"radmiraal/emberjs": "dev-master",
		"radmiraal/couchdb": "dev-doctrine",
		"doctrine/dbal": "2.3.1",
		"doctrine/orm": "2.3.1",
		"typo3/formbuilder": "dev-master",
		"typo3/queue-redis": "*",
		"react/event-loop": "*",
		"typo3/usermanagement": "dev-master"
	},
	"require-dev": {
		"typo3/buildessentials": "dev-master",
		"typo3/kickstart": "dev-master",
		"phpunit/phpunit": "3.7.*",
		"typo3-qa/typo3flow": "dev-master",
		"mikey179/vfsstream": "1.1.*",
		"beech/style": "dev-development",
		"phpmd/phpmd": "1.4.*",
		"phpcd/phpcd": "dev-master",
		"pdepend/pdepend": "dev-master"
	},
	"minimum-stability": "dev",
	"scripts": {
		"post-update-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
		"post-install-cmd": "TYPO3\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall"
	}
}